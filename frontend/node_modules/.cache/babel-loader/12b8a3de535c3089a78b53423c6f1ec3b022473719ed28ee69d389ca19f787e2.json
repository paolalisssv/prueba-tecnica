oken: this.token }} )\n      .then( data =>{\n          if(data.status == 200){\n            this.content=data.data[0].content\n            this.type=data.data[0].type\n            this.title=data.data[0].title\n            this.topic=data.data[0].topic\n            this.user= data.data[0].user\n            this.id=data.data[0]._id\n          }else{\n          console.log(data)\n            this.error = true;\n            this.error_msg = data.data.message;\n          }\n      })\n  },\n  methods:{\n    getFile(event){\n      this.imagen = event.target.files[0];\n    },\n    edit(){\n        this.token= localStorage.token;\n        var data = new  FormData();\n        data.append('content', this.imagen);\n        data.append('title', this.title);\n        data.append('topic', this.topic);\n        data.append('type', this.type);\n        data.append('user', localStorage._id);\n        this.token= localStorage.token;\n\n        axios.put('http://localhost:3000/api/content/update', data, { headers: { token: this.token }} )\n        .then( data =>{\n          console.log(data)\n           if(data.status == 200){\n             this.$router.push('/library');\n           }else{\n             this.error = true;\n             this.error_msg = data.data.message;\n           }\n        })\n        .catch(function () {\n         console.log(\"error\")\n         //this.error = ref('does it work?');\n\n        })\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\np{\n  margin:0px;\n}\n</style>\n"],"mappings":";AA0BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE,CAEZ,CAAC;EACAC,IAAI,EAAE,SAAAA,CAAA,EAAU;IACf,OAAO;MACLC,OAAO,EAAC,IAAI;MACZC,KAAK,EAAC,EAAE;MACRC,KAAK,EAAC,EAAE;MACRC,KAAK,EAAC,EAAE;MACRC,IAAI,EAAC,EAAE;MACPC,MAAM,EAAC,