/high-power-prueba-tecnica/frontend/src/views/AddNewContentView.vue"],"sourcesContent":["<template>\n  <div class=\"hello\">\n  <h4>Agregar nuevo contenido</h4>\n   <form v-on:submit.prevent=\"addNew\">\n      <input type=\"text\" id=\"title\" class=\"fadeIn second\" name=\"title\" placeholder=\"Titulo\" v-model=\"title\">\n      <p>Tematica</p>\n       <select v-model=\"topic\">\n        <option disabled value=\"\">Tematica</option>\n        <option v-for=\"top in topics\" :key=\"top.name\" value=\"top._id\">\n          {{ top.name }}\n        </option>\n      </select>\n      <p>Tipo de archivo</p>\n      <select v-model=\"type\">\n        <option disabled value=\"\">Tipo de archivo</option>\n        <option>image/png</option>\n        <option>image/jpeg</option>\n        <option>text/plain</option>\n      </select>\n      <input type=\"file\"  id=\"content\" class=\"fadeIn second\" name=\"content\" placeholder=\"Content\">\n      <input type=\"submit\" class=\"fadeIn fourth button\" value=\"Subir contenido\">\n    </form>\n\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'AddNewContentView',\n  components: {\n\n  },\n   data: function(){\n    return {\n      imagen:null,\n      token:\"\",\n    }\n  },\n  mounted() {\n    this.token= localStorage.token;\n    axios.get('http://localhost:3000/api/topics/list/', { headers: { token: this.token }} )\n      .then( data =>{\n          if(data.status == 200){\n          console.log(\"dataaa\",data)\n            this.topics = data.data\n          }else{\n          console.log(data)\n            this.error = true;\n            this.error_msg = data.data.message;\n          }\n      })\n  },\n  methods:{\n    getImage(event){\n      this.imagen = event.target.files[0];\n    },\n    addNew(){\n        this.token= localStorage.token;\n        var data = new  FormData();\n        data.append('content', this.imagen);\n        data.append('title', this.title);\n        data.append('topic', this.topic);\n        data.append('type', this.type);\n        data.append('user', localStorage._id);\n        this.token= localStorage.token;\n\n        axios.post('http://localhost:3000/api/content/register', data, { headers: { token: this.token }} )\n        .then( data =>{\n          console.log(data)\n           if(data.status == 200){\n             this.$router.push('/library');\n           }else{\n             this.error = true;\n             this.error_msg = data.data.message;\n           }\n        })\n        .catch(function () {\n         console.log(\"error\")\n         //this.error = ref('does it work?');\n\n        })\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\np{\n  margin:0px;\n}\n</style>\n"],"mappings":";AA2BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,UAAU,EAAE,CAEZ,CAAC;EACAC,IAAI,EAAE,SAAAA,CAAA,EAAU;IACf,OAAO;MACLC,MAAM,EAAC,IAAI;MACXC,KAAK,EAAC;IACR;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACD,KAAK,GAAEE,YAAY,CAACF,KAAK;IAC9BL,KAAK,CAACQ,GAAG,CAAC,wCAAwC,EAAE;MAAEC,OAAO,EAAE;QAAEJ,KAAK,EAAE,IAAI,CAACA;MAAM;IAAC,CAAE,EACnFK,IAAI,CAAEP,IAAG,IAAI;MACV,IAAGA,IAAI,CAACQ,MAAK,IAAK,GAAG,EAAC;QACtBC,OAAO,CAACC,GAAG,CAAC,QAAQ,EAACV,IAAI;QACvB,IAAI,CAACW,MAAK,GAAIX,IAAI,CAACA,IAAG;MACxB,CAAC,MAAI;QACLS,OAAO,CAACC,GAAG,CAACV,IAAI;QACd,IAAI,CAACY,KAAI,GAAI,IAAI;QACjB,IAAI,CAACC,SAAQ,GAAIb,IAAI,CAACA,IAAI,CAACc,OAAO;MACpC;IACJ,CAAC;EACL,CAAC;EACDC,OAAO,EAAC;IACNC,QAAQA,CAACC,KAAK,EAAC;MACb,IAAI,CAAChB,MAAK,GAAIgB,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IACrC,CAAC;IACDC,MAAMA,CAAA,EAAE;MACJ,IAAI,CAAClB,KAAK,GAAEE,YAAY,CAACF,KAAK;MAC9B,IAAIF,IAAG,GAAI,IAAKqB,QAAQ,CAAC,CAAC;MAC1BrB,IAAI,CAACsB,MAAM,CAAC,SAAS,EAAE,IAAI,CAACrB,MAAM,CAAC;MACnCD,IAAI,CAACsB,MAAM,CAAC,OAAO,EAAE,IAAI,CAACC,KAAK,CAAC;MAChCvB,