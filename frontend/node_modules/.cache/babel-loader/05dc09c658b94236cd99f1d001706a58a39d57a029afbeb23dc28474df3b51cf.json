   }\n  },\n  mounted() {\n    this.token= localStorage.token;\n    axios.get('http://localhost:3000/api/topics/list/', { headers: { token: this.token }} )\n      .then( data =>{\n          if(data.status == 200){\n          console.log(\"dataaa\",data)\n            this.topics = data.data\n          }else{\n          console.log(data)\n            this.error = true;\n            this.error_msg = data.data.message;\n          }\n      })\n  },\n  methods:{\n    getFile(event){\n      this.imagen = event.target.files[0];\n      this.imageType= event.target.files[0].type\n    },\n    addNew(){\n      if(this.imageType !== this.type){\n        this.errorType = true\n      }else{\n        this.token= localStorage.token;\n        var data = new  FormData();\n        data.append('content', this.imagen);\n        data.append('title', this.title);\n        data.append('topic', this.topic);\n        data.append('type', this.type);\n        data.append('user', localStorage._id);\n        this.token= localStorage.token;\n\n        console.log(data)\n\n        axios.post('http://localhost:3000/api/content/register', data, { headers: { token: this.token }} )\n        .then( data =>{\n          console.log(data)\n           if(data.status == 200){\n             this.$router.push('/library');\n           }else{\n             this.error = true;\n             this.error_msg = data.data.message;\n           }\n        })\n        .catch(function () {\n         console.log(\"error\")\n         //this.error = ref('does it work?');\n\n        })\n      }\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\np{\n  margin:0px;\n}\n</style>\n"],"mappings":";AA+BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,mBAAmB;EACzBC,UAAU,EAAE,CAEZ,CAAC;EACAC,IAAI,EAAE,SAAAA,CAAA,