{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nimport { useRoute } from 'vue-router';\nexport default {\n  name: 'EditContentView',\n  components: {},\n  data: function () {\n    return {\n      previewUrl: '',\n      content: null,\n      token: \"\",\n      title: \"\",\n      topic: \"\",\n      type: \"\",\n      topics: [],\n      id: \"\",\n      errorEmpty: false,\n      errorType: false,\n      contentType: \"\"\n    };\n  },\n  mounted() {\n    this.token = localStorage.token;\n    axios.get('http://localhost:3000/api/topics/list/', {\n      headers: {\n        token: this.token\n      }\n    }).then(data => {\n      if (data.status == 200) {\n        console.log(\"dataaa\", data);\n        this.topics = data.data;\n      } else {\n        console.log(data);\n        this.error = true;\n        this.error_msg = data.data.message;\n      }\n    });\n    this.token = localStorage.token;\n    const route = useRoute();\n    this.id = route.params.id;\n    axios.get(`http://localhost:3000/api/content/show/${this.id}`, {\n      headers: {\n        token: this.token\n      }\n    }).then(data => {\n      if (data.status == 200) {\n        if (data.data[0].type === '' || data.data[0].type === '') {\n          this.previewUrl = 'http://localhost:3000/api/content/' + data.data[0].content;\n        }\n        this.content = data.data[0].content;\n        this.type = data.data[0].type;\n        this.title = data.data[0].title;\n        this.topic = data.data[0].topic;\n        this.user = data.data[0].user;\n        this.id = data.data[0]._id;\n        console.log(data.data[0].content);\n      } else {\n        console.log(data);\n        this.error = true;\n        this.error_msg = data.data.message;\n      }\n    });\n  },\n  methods: {\n    getFile(event) {\n      this.content = event.target.files[0];\n      this.contentType = event.target.files[0].type;\n      const reader = new FileReader();\n      const that = this;\n      reader.onload = function (event) {\n        that.previewUrl = event.target.result;\n      };\n      reader.readAsDataURL(this.imagen);\n    },\n    edit() {\n      if (this.contentType !== this.type) {\n        this.errorType = true;\n      } else if (this.type === \"\" || this.content === null || this.title === \"\" || this.topic === \"\") {\n        this.errorEmpty = true;\n      } else {\n        var data = new FormData();\n        data.append('content', this.content);\n        data.append('title', this.title);\n        data.append('topic', this.topic);\n        data.append('type', this.type);\n        data.append('user', localStorage._id);\n        data.append('_id', this.id);\n        this.token = localStorage.token;\n        axios.put('http://localhost:3000/api/content/update', data, {\n          headers: {\n            token: this.token\n          }\n        }).then(data => {\n          console.log(data);\n          if (data.status == 200) {\n            this.$router.push('/library');\n          } else {\n            this.error = true;\n            this.error_msg = data.data.message;\n          }\n        }).catch(function () {\n          console.log(\"error\");\n          //this.error = ref('does it work?');\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","useRoute","name","components","data","previewUrl","content","token","title","topic","type","topics","id","errorEmpty","errorType","contentType","mounted","localStorage","get","headers","then","status","console","log","error","error_msg","message","route","params","user","_id","methods","getFile","event","target","files","reader","FileReader","that","onload","result","readAsDataURL","imagen","edit","FormData","append","put","$router","push","catch"],"sources":["/Users/paolasantollovargas/Documents/high-power-prueba-tecnica/frontend/src/views/EditContentView.vue"],"sourcesContent":["<template>\n  <div class=\"hello\">\n  <h4>Editar contenido</h4>\n   <form v-on:submit.prevent=\"edit\">\n      <input type=\"text\" id=\"title\" class=\"fadeIn second\" name=\"title\" placeholder=\"Titulo\" v-model=\"title\">\n      <p>Tematica</p>\n       <select v-model=\"topic\">\n        <option disabled value=\"\">Tematica</option>\n        <option v-for=\"top in topics\" :key=\"top.name\" v-bind:value=\"top._id\">\n          {{ top.name }}\n        </option>\n      </select>\n      <p>Tipo de archivo</p>\n      <select v-model=\"type\">\n        <option disabled>Tipo de archivo</option>\n        <option value=\"image/png\">image/png</option>\n        <option value=\"image/jpeg\">image/jpeg</option>\n        <option value=\"text/plain\">text/plain</option>\n      </select>\n      <input type=\"file\"  id=\"content\" class=\"fadeIn second\" name=\"content\" placeholder=\"Content\" @change=\"getFile\" >\n      <div v-if=\"this.type==='image/png' || this.type==='image/jpeg'\">\n        <img :src=\"previewUrl\" v-if=\"previewUrl\">\n        <p v-else>No image...</p>\n      </div>\n      <input type=\"submit\" class=\"fadeIn fourth button\" value=\"Editar contenido\">\n\n      <p v-if=\"errorType\">El archivo y el tipo de archivo debe coincidir</p>\n      <p v-if=\"errorEmpty\">Todos los campos deben estar llenos</p>\n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\nimport { useRoute } from 'vue-router'\n\nexport default {\n  name: 'EditContentView',\n  components: {\n\n  },\n   data: function(){\n    return {\n      previewUrl: '',\n      content:null,\n      token:\"\",\n      title:\"\",\n      topic:\"\",\n      type:\"\",\n      topics:[],\n      id:\"\",\n      errorEmpty: false,\n      errorType: false,\n      contentType:\"\"\n    }\n  },\n  mounted() {\n    this.token= localStorage.token;\n    axios.get('http://localhost:3000/api/topics/list/', { headers: { token: this.token }} )\n      .then( data =>{\n          if(data.status == 200){\n          console.log(\"dataaa\",data)\n            this.topics = data.data\n          }else{\n          console.log(data)\n            this.error = true;\n            this.error_msg = data.data.message;\n          }\n    })\n    this.token= localStorage.token;\n    const route = useRoute()\n    this.id = route.params.id\n    axios.get(`http://localhost:3000/api/content/show/${this.id}`, { headers: { token: this.token }} )\n      .then( data =>{\n          if(data.status == 200){\n            if(data.data[0].type === '' || data.data[0].type ===''){\n              this.previewUrl ='http://localhost:3000/api/content/'+data.data[0].content\n            }\n            this.content=data.data[0].content\n            this.type=data.data[0].type\n            this.title=data.data[0].title\n            this.topic=data.data[0].topic\n            this.user= data.data[0].user\n            this.id=data.data[0]._id\n            console.log(data.data[0].content)\n          }else{\n          console.log(data)\n            this.error = true;\n            this.error_msg = data.data.message;\n          }\n      })\n  },\n  methods:{\n    getFile(event){\n      this.content = event.target.files[0];\n      this.contentType= event.target.files[0].type\n\n      const reader = new FileReader()\n      const that = this\n      reader.onload = function (event) {\n        that.previewUrl = event.target.result\n      }\n      reader.readAsDataURL(this.imagen)\n    },\n    edit(){\n\n     if(this.contentType !== this.type){\n        this.errorType = true\n      }else if(this.type === \"\" || this.content === null || this.title === \"\" || this.topic === \"\" ){\n        this.errorEmpty = true\n      }\n      else{\n\n        var data = new  FormData();\n        data.append('content', this.content);\n        data.append('title', this.title);\n        data.append('topic', this.topic);\n        data.append('type', this.type);\n        data.append('user', localStorage._id);\n        data.append('_id', this.id);\n        this.token= localStorage.token;\n\n        axios.put('http://localhost:3000/api/content/update', data, { headers: { token: this.token }} )\n        .then( data =>{\n          console.log(data)\n           if(data.status == 200){\n             this.$router.push('/library');\n           }else{\n             this.error = true;\n             this.error_msg = data.data.message;\n           }\n        })\n        .catch(function () {\n         console.log(\"error\")\n         //this.error = ref('does it work?');\n\n        })\n      }\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\np{\n  margin:0px;\n}\nimg{\n width:300px;\n height: 300px;\n}\n</style>\n"],"mappings":";AAiCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,YAAW;AAEpC,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE,CAEZ,CAAC;EACAC,IAAI,EAAE,SAAAA,CAAA,EAAU;IACf,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAC,IAAI;MACZC,KAAK,EAAC,EAAE;MACRC,KAAK,EAAC,EAAE;MACRC,KAAK,EAAC,EAAE;MACRC,IAAI,EAAC,EAAE;MACPC,MAAM,EAAC,EAAE;MACTC,EAAE,EAAC,EAAE;MACLC,UAAU,EAAE,KAAK;MACjBC,SAAS,EAAE,KAAK;MAChBC,WAAW,EAAC;IACd;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACT,KAAK,GAAEU,YAAY,CAACV,KAAK;IAC9BP,KAAK,CAACkB,GAAG,CAAC,wCAAwC,EAAE;MAAEC,OAAO,EAAE;QAAEZ,KAAK,EAAE,IAAI,CAACA;MAAM;IAAC,CAAE,EACnFa,IAAI,CAAEhB,IAAG,IAAI;MACV,IAAGA,IAAI,CAACiB,MAAK,IAAK,GAAG,EAAC;QACtBC,OAAO,CAACC,GAAG,CAAC,QAAQ,EAACnB,IAAI;QACvB,IAAI,CAACO,MAAK,GAAIP,IAAI,CAACA,IAAG;MACxB,CAAC,MAAI;QACLkB,OAAO,CAACC,GAAG,CAACnB,IAAI;QACd,IAAI,CAACoB,KAAI,GAAI,IAAI;QACjB,IAAI,CAACC,SAAQ,GAAIrB,IAAI,CAACA,IAAI,CAACsB,OAAO;MACpC;IACN,CAAC;IACD,IAAI,CAACnB,KAAK,GAAEU,YAAY,CAACV,KAAK;IAC9B,MAAMoB,KAAI,GAAI1B,QAAQ,CAAC;IACvB,IAAI,CAACW,EAAC,GAAIe,KAAK,CAACC,MAAM,CAAChB,EAAC;IACxBZ,KAAK,CAACkB,GAAG,CAAE,0CAAyC,IAAI,CAACN,EAAG,EAAC,EAAE;MAAEO,OAAO,EAAE;QAAEZ,KAAK,EAAE,IAAI,CAACA;MAAM;IAAC,CAAE,EAC9Fa,IAAI,CAAEhB,IAAG,IA