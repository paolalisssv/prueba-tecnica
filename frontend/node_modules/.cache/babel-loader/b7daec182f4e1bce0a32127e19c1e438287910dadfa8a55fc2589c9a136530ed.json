rorType = true\n        }\n      }\n      if(this.type === \"\" || this.content === null || this.title === \"\" || this.topic === \"\" ){\n        this.errorEmpty = true\n      }\n      else{\n        var data = new  FormData();\n        if(this.fileChange === true){\n          data.append('content', this.content);\n          data.append('type', this.type);\n\n        }\n        data.append('title', this.title);\n        data.append('topic', this.topic);\n        data.append('user', localStorage._id);\n        data.append('_id', this.id);\n        this.token= localStorage.token;\n\n        axios.put('http://localhost:3000/api/content/update', data, { headers: { token: this.token }} )\n        .then( data =>{\n          console.log(data)\n           if(data.status == 200){\n             this.$router.push('/library');\n           }else{\n             this.error = true;\n             this.error_msg = data.data.message;\n           }\n        })\n        .catch(function () {\n         console.log(\"error\")\n         //this.error = ref('does it work?');\n\n        })\n      }\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\np{\n  margin:0px;\n}\nimg{\n width:300px;\n height: 300px;\n}\n</style>\n"],"mappings":";AAiCA,OAAOA,KAAI,MAAO,OAAO;AACzB,SAASC,QAAO,QAAS,YAAW;AAEpC,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE,CAEZ,CAAC;EACAC,IAAI,EAAE,SAAAA,CAAA,EAAU;IACf,OAAO;MACLC,UAAU,EAAE,EAAE;MACdC,OAAO,EAAC,IAAI;MACZC,KAAK,EAAC,EAAE;MACRC,KAAK,EAAC,EAAE;MACRC,KAAK,EAAC,EAAE;MACRC,IAAI,EAAC,EAAE;MACPC,MAAM,EAAC,EAAE;MACTC,EAAE,EAAC,EAAE;MACLC,UAAU,EAAE,KAAK;MACjBC,SAAS,EAAE,KAAK;MAChBC,WAAW,EAAC,EAAE;MACdC,UAAU,EAAE;IACd;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACV,KAAK,GAAEW,YAAY,CAACX,KAAK;IAC9BP,KAAK,CAACmB,GAAG,CAAC,wCAAwC,EAAE;MAAEC,OAAO,EAAE;QA