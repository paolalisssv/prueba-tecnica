iew',\n  components: {\n\n  },\n   data: function(){\n    return {\n      name:\"\",\n      imagen : null,\n    }\n  },\n  methods:{\n    getImage(event){\n      this.imagen = event.target.files[0];\n    },\n    addNew(){\n        this.token= localStorage.token;\n        var data = new  FormData();\n        data.append('cover', this.imagen);\n        data.append('name', this.name);\n\n        axios.post('http://localhost:3000/api/topics/register', data, { headers: { token: this.token }} )\n        .then( data =>{\n          console.log(data)\n           if(data.status == 200){\n             this.$router.push('/login');\n           }else{\n             this.error = true;\n             this.error_msg = data.data.message;\n           }\n        })\n        .catch(function () {\n         console.log(\"error\")\n         //this.error = ref('does it work?');\n\n        })\n    }\n  }\n\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\n</style>\n"],"mappings":";AAaA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE,CAEZ,CAAC;EACAC,IAAI,EAAE,SAAAA,CAAA,EAAU;IACf,OAAO;MACLF,IAAI,EAAC,EAAE;MACPG,MAAK,EAAI;IACX;EACF,CAAC;EACDC,OAAO,EAAC;IACNC,QAAQA,CAACC,KAAK,EAAC;MACb,IAAI,CAACH,MAAK,GAAIG,KAAK,CAACC,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;IACrC,CAAC;IACDC,MAAMA,CAAA,EAAE;MACJ,IAAI,CAACC,KAAK,GAAEC,YAAY,CAACD,KAAK;MAC9B,IAAIR,IAAG,GAAI,IAAKU,QAAQ,CAAC,CAAC;MAC1BV,IAAI,CAACW,MAAM,CAAC,OAAO,EAAE,IAAI,CAACV,MAAM,CAAC;MACjCD,IAAI,CAACW,MAAM,CAAC,MAAM,EAAE,IAAI,CAACb,IAAI,CAAC;MAE9BD,KAAK,CAACe,IAAI,CAAC,2CAA2C,EAAEZ,IAAI,EAAE;QAAEa,OAAO,EAAE;UAAEL,KAAK,EAAE,IAAI,CAACA;QAAM;MAAC,CAAE,EAC/FM,IAAI,CAAEd,IAAG,IAAI;QACZe,OAAO,CAACC,GAAG,CAAChB,IAAI;QACf,IAAGA,IAAI,CAACiB,MAAK,IAAK,GAAG,EAAC;UACpB,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,QAAQ,CAAC;QAC7B,CAAC,MAAI;UACH,IA