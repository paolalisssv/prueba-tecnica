pend('content', this.imagen);\n        }\n        data.append('title', this.title);\n        data.append('topic', this.topic);\n        data.append('type', this.type);\n        data.append('user', localStorage._id);\n        this.token = localStorage.token;\n        console.log(data);\n        axios.post('http://localhost:3000/api/content/register', data, {\n          headers: {\n            token: this.token\n          }\n        }).then(data => {\n          console.log(data);\n          if (data.status == 200) {\n            this.$router.push('/library');\n          } else {\n            this.error = true;\n            this.error_msg = data.data.message;\n          }\n        }).catch(function () {\n          console.log(\"error\");\n          //this.error = ref('does it work?');\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["axios","name","components","data","imagen","token","title","topic","type","topics","errorType","errorEmpty","imageType","previewUrl","fileContent","mounted","localStorage","get","headers","then","status","console","log","error","error_msg","message","methods","getFile","event","target","files","reader","FileReader","that","onload","result","readAsText","file","readAsDataURL","addNew","image","FormData","append","_id","post","$router","push","catch"],"sources":["/Users/paolasantollovargas/Documents/high-power-prueba-tecnica/frontend/src/views/AddNewContentView.vue"],"sourcesContent":["<template>\n  <div class=\"hello\">\n  <h4>Agregar nuevo contenido</h4>\n   <form v-on:submit.prevent=\"addNew\">\n      <input type=\"text\" id=\"title\" class=\"fadeIn second\" name=\"title\" placeholder=\"Titulo\" v-model=\"title\">\n      <p>Tematica</p>\n       <select v-model=\"topic\">\n        <option disabled value=\"\">Tematica</option>\n        <option v-for=\"top in topics\" :key=\"top.name\" v-bind:value=\"top._id\">\n          {{ top.name }}\n        </option>\n      </select>\n      <p>Tipo de archivo</p>\n      <select v-model=\"type\">\n        <option disabled>Tipo de archivo</option>\n        <option value=\"image/png\">image/png</option>\n        <option value=\"image/jpeg\">image/jpeg</option>\n        <option value=\"text/plain\">text/plain</option>\n      </select>\n      <input type=\"file\"  id=\"content\" class=\"fadeIn second\" name=\"content\" placeholder=\"Content\" @change=\"getFile\" >\n      <div v-if=\"this.type==='image/png' || this.type==='image/jpeg'\">\n        <img :src=\"previewUrl\" v-if=\"previewUrl\">\n        <p v-else>No image...</p>\n      </div>\n      <div v-if=\"this.type==='text/plain'\">\n        <p>{{this.fileContent}}</p>\n        <p>oliiii</p>\n      </div>\n      <input type=\"submit\" class=\"fadeIn fourth button\" value=\"Subir contenido\">\n      \n      <p v-if=\"errorType\">El archivo y el tipo de archivo debe coincidir</p>\n      <p v-if=\"errorEmpty\">Todos los campos deben estar llenos</p>\n    \n    </form>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios';\n\nexport default {\n  name: 'AddNewContentView',\n  components: {\n\n  },\n   data: function(){\n    return {\n      imagen:null,\n      token:\"\",\n      title:\"\",\n      topic:\"\",\n      type:\"\",\n      topics:[],\n      errorType: false,\n      errorEmpty: false,\n      imageType:\"\",\n      previewUrl: '',\n      fileContent:\"\"\n    }\n  },\n  mounted() {\n    this.token= localStorage.token;\n    axios.get('http://localhost:3000/api/topics/list/', { headers: { token: this.token }} )\n      .then( data =>{\n          if(data.status == 200){\n          console.log(\"dataaa\",data)\n            this.topics = data.data\n          }else{\n          console.log(data)\n            this.error = true;\n            this.error_msg = data.data.message;\n          }\n      })\n  },\n  methods:{\n    getFile(event){\n      this.imagen = event.target.files[0];\n      this.imageType= event.target.files[0].type;\n\n      const reader = new FileReader()\n      const that = this\n      reader.onload = function (event) {\n        if(event.target.files[0].type==='text/plain'){\n          that.fileContent = event.target.result\n        console.log(this.fileContent)\n\n        }else{\n          that.previewUrl = event.target.result\n        }\n      }\n      if(event.target.files[0].type==='text/plain'){\n        reader.readAsText(file)\n      }else{\n        reader.readAsDataURL(this.imagen)\n      }\n    },\n    addNew(){\n      if(this.imageType !== this.type){\n        this.errorType = true\n      }else if(this.type === \"\" || this.image === null || this.title === \"\" || this.topic === \"\" ){\n        this.errorEmpty = true\n      }\n      else{\n        this.token= localStorage.token;\n        var data = new  FormData();\n        if(this.type==='text/plain'){\n          data.append('content', this.fileContent);\n\n        }else{\n          data.append('content', this.imagen);\n        }\n        data.append('title', this.title);\n        data.append('topic', this.topic);\n        data.append('type', this.type);\n        data.append('user', localStorage._id);\n        this.token= localStorage.token;\n\n        console.log(data)\n\n        axios.post('http://localhost:3000/api/content/register', data, { headers: { token: this.token }} )\n        .then( data =>{\n          console.log(data)\n           if(data.status == 200){\n             this.$router.push('/library');\n           }else{\n             this.error = true;\n             this.error_msg = data.data.message;\n           }\n        })\n        .catch(function () {\n         console.log(\"error\")\n         //this.error = ref('does it work?');\n\n        })\n      }\n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\nh3 {\n  margin: 40px 0 0;\n}\nimg{\n width:300px;\n height: 300px;\n}\nul {\n  list-style-type: none;\n  padding: 0;\n}\nli {\n  display: inline-block;\n  margin: 0 10px;\n}\na {\n  color: #42b983;\n}\np{